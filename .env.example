# Copy to `.env` and fill in values.

# Pterodactyl Panel base URL (no trailing slash preferred)
PANEL_URL=https://panel.example.com

# Pterodactyl *Application* API key (admin): typically starts with "ptla_"
PANEL_TOKEN=ptla_REPLACE_ME

# Optional
PANEL_TIMEOUT=30
PANEL_VERIFY_SSL=true
PANEL_USER_AGENT=PterodactylMCP/0.1

